{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard", "eslint-config-ali/typescript/node"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "@typescript-eslint/ban-ts-comment": 0,
        "@typescript-eslint/member-ordering": 0,
        "max-len": 0
    }
}
